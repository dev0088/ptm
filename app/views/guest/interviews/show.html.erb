<%- if @interview.ended? %>
  <%= render 'completed_interview' %>
<%- else %>
	<%- if @interview.posting? %>
  	<%= render 'interviews/posted_interview' %>
  <%- elsif @interview.guest_solicited? && guest? %>
  	<%= render 'interviews/solicited/solicited_interview' %>
  <%- else %>
	  <%= render 'interviews/upcoming_interview' %>
	 <%- end %>
<%- end %>
